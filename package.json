      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@svgr/cli": "^5.5.0",
    "bootstrap": "^4.6.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "lint-staged": "^10.5.4",
    "next": "10.0.6",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.32.7"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0"
  }
