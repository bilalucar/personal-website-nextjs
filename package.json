{
  "name": "personal-website-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "icon": "svgr icons -d components/icons --icon",
    "prettier": "prettier --write \"./**/*.js\""
  },
  "pre-commit": [
